!MarsRoverObserverTest methodsFor: 'tests' stamp: 'TM 6/5/2023 13:23:18'!
test03MarsRoverSupportsMultipleObservers

	| rover observer1 observer2 result |
	
	rover := MarsRover at: 0@0 heading: MarsRoverHeadingNorth.
	
	observer1 := MarsRoverHeadingAndPositionWindow new.
	observer2 := MarsRoverHeadingAndPositionWindow new.
	
	rover addObserver: observer1.
	rover addObserver: observer2.
	
	rover process: 'fr'.
	
	result := OrderedCollection new.
	result add: 0@1.
	result add: 'East'.
	
	self assert: 'East' equals: observer1 heading.
	self assert: 'East' equals: observer2 heading.! !

